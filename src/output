            url?: string;
            previewUrl?: string;
            provider?: string;
            provider_metadata?: unknown;
            related?: {
              data?: {
                  id?: number;
                  attributes?: Record<string, never>;
                }[];
            };
            folder?: {
              data?: {
                id?: number;
                attributes?: Record<string, never>;
              };
            };
