        }[];
    };
    SharedSeoComponent: {
      id?: number;
      metaTitle?: string;
      metaDescription?: string;
      metaImage?: {
        data?: {
          id?: number;
          attributes?: {
            name?: string;
            alternativeText?: string;
            caption?: string;
            width?: number;
            height?: number;
            formats?: unknown;
            hash?: string;
            ext?: string;
            mime?: string;
            /** Format: float */
