                        name={_item?.attributes?.name || ""}
                        id={_item.id}
                        description={_item.attributes?.description || ""}
                        rating={_item.attributes?.rating || 0}
                        key={_item.id}
                      />
                    </div>
                  </div>
                </SwiperSlide>
              );
            })}
        </Swiper>
      </div>
    </div>
  );
