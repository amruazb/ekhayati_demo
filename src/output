interface IStats {
  averageScore: number;
  reviewsCount: number | null;
}

interface IAuthor {
  username: string;
  email: string;
  id: number;
}

interface IReview {
  id: number;
  createdAt: string;
  comment: string | null;
  author: IAuthor | null;
  score: number;
}

