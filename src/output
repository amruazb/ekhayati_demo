            query: query ? query : {},
          }}
          scroll={true}
          prefetch={true}
          className="relative"
        >
          {customizable ? <Chip className={`absolute z-10 ${locale === "en" ? 'left-[10px]' : 'right-[10px]'} top-[10px] text-[10px] rounded-md bg-[#012828] text-white`} variant="shadow">{t("customizable")}</Chip> :  <></>}
          <Image
            priority={true}
            unoptimized
            src={image || ""}
            width="0"
            height="0"
            sizes="100vw"
