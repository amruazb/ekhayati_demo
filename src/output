export const extractError = (error: any) => {
  //@ts-ignore
  if (error?.error?.details?.errors?.[0]?.message) {
    //@ts-ignore
    return error?.error.details.errors[0].message;
  }

  if (error?.error?.message) {
    return error?.error?.message;
  }
}

