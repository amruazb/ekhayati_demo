          // modules={[Pagination, Autoplay]}
          className="tailor-home-carousel"
          breakpoints={{
            600: {
              slidesPerView: 1,
              spaceBetween: 20,
              centeredSlides: true,
            },
            800: {
              slidesPerView: 2,
              spaceBetween: 20,
              centeredSlides: true,
            },
            1500: {
              slidesPerView: 3,
              spaceBetween: 20,
              centeredSlides: false,
            },
            3000: {
              slidesPerView: 5,
              spaceBetween: 20,
              centeredSlides: false,
            },
          }}
